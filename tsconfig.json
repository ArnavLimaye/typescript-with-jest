  "compilerOptions": {
    "module": "system",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "outFile": "./built/local/tsc.js",
    "sourceMap": true
  },
  "include": ["src"],
  "exclude": ["./node_modules", "**/*.spec.ts"]
}
